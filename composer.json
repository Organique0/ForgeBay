{
  "name": "karlverger/starterkit",
  "type": "project",
  "description": "Starter kit for Laravel11, Jetstream, Inertia, React, Chakra and swr applications.",
  "keywords": [
    "laravel",
    "framework",
    "jetstream",
    "inertia",
    "react",
    "chakra ui",
    "swr"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "filament/filament": "^3.2",
    "http-interop/http-factory-guzzle": "^1.2",
    "inertiajs/inertia-laravel": "^1.0",
    "laravel/framework": "^11.9",
    "laravel/horizon": "^5.30",
    "laravel/jetstream": "^5.3",
    "laravel/prompts": "^0.3.3",
    "laravel/pulse": "^1.3",
    "laravel/reverb": "^1.0",
    "laravel/sanctum": "^4.0",
    "laravel/scout": "^10.13",
    "laravel/socialite": "^5.18",
    "laravel/tinker": "^2.9",
    "livewire/livewire": "^3.5",
    "predis/predis": "^2.3",
    "spatie/laravel-permission": "^6.10",
    "teamtnt/laravel-scout-tntsearch-driver": "^15.0",
    "tightenco/ziggy": "^2.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.41",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0.1"
  },
  "authors": [
    {
      "name": "Karl Verger",
      "email": "karl.verger@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
